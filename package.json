{
  "name": "bw-fullstck-reference-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "local": "ng serve --proxy-config proxy.conf.local.json --port 4200",
    "clean:api-sdk": "rimraf *.tgz ./sdk/bw-fullstack-reference-api-sdk",
    "generate-core-api-sdk:local": "npm run clean:api-sdk && openapi-generator-cli generate -g typescript-angular -i https://eoladipupo.byteproducts.com/api/v3/api-docs -o ./sdk/bw-fullstack-reference-api-sdk --additional-properties npmName=bw-fullstack-reference-api-sdk,snapshot=true,ngVersion=9.0.1,fileNaming=kebab-case,npmVersion=1.0.0,useSingleRequestParameter=true --type-mappings=DateTime=string"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/localize": "^15.1.1",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "angular-archwizard": "^7.0.0",
    "bootstrap": "^5.2.3",
    "font-awesome": "^4.7.0",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "google-libphonenumber": "^3.2.7",
    "intl-tel-input": "^14.1.0",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.0.2",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-intl-tel-input": "3.1.3",
    "ngx-toastr": "^14.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
